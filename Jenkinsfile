pipeline{
  agent any
  stages{
    stage("init"){
      steps{
        echo "the init stage is running......"
      }
    }
    stage("build"){
      steps{
        echo "the build stage is running......"
      }
    }
    stage("test"){
      steps{
        echo "the test stage is running......"
      }
    }
    stage("deploy"){
      steps{
        echo "the deploy stage is running......"
      }
    }
  }
}
